  "author": "Abhirup Kumar Bhowmick",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.61",
    "client-only": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.12.0",
    "server-only": "^0.0.1",
    "tslib": "^2.6.2"
  }
}
