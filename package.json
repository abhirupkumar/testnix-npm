{
  "name": "testnix",
  "version": "1.0.3",
  "description": "This package helps you to track the events on your website and do split testing.",
  "main": "./dist/index.js",
  "types": "./dist/server/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npx rollup -c"
  },
  "keywords": [
    "ab-testing",
